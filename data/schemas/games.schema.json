{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "definitions": {
        "gameId": {
            "type": "string",
            "enum": ["ykw1", "ykw2", "ykw3"]
        }
    },
    "type": "object",
    "patternProperties": {
        "^[a-z]+$": {
            "type": "object",
            "required": ["id", "name", "image"],
            "properties": {
                "id": { "$ref": "#/definitions/gameId" },
                "name": { "type": "string" },
                "image": { "type": "string" }
            }
        }
    }
}