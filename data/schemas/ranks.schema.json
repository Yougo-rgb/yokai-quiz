{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "definitions": {
        "rankId": {
            "type":"string",
            "enum": ["e", "d", "c", "b", "a", "s", "boss-rank"]
        }
    },
    "type": "object",
    "patternProperties": {
        "^[a-z]+$": {
            "type": "object",
            "required": ["id", "name", "image"],
            "properties": {
                "id": { "$ref": "#/definitions/rankId" },
                "name": { "type": "string" },
                "image": { "type": "string" }
            }
        }
    }
}